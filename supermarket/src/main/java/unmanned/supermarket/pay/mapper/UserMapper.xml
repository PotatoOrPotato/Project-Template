<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="unmanned.supermarket.pay.mapper.UserMapper">

    <!-- 根据用户id查询用户记录条数-->
    <select id="findUserInfoById" parameterType="String" resultType="int">
        select count(*) from gggg where gid = #{_parameter}
    </select>

</mapper>